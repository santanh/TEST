<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns="http://www.tibco.com/pe/DeployedVarsType" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:ns2="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:ns1="http://www.tibco.com/pe/EngineTypes" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <pd:name>Processes/SubProcesses/SSCH/SSCHOutput.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="OrderStr" type="xsd:string"/>
                    <xsd:element name="FileName" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>52</pd:startX>
    <pd:startY>107</pd:startY>
    <pd:returnBindings>
        <root>
            <xsl:choose>
                <xsl:when test="count($List-File/pfx:ListFilesActivityOutput/files/fileInfo) = 0">
                    <SSCHString>
                        <xsl:value-of select="concat($Render-Header/Output/dataString, $Start/root/OrderStr)"/>
                    </SSCHString>
                </xsl:when>
                <xsl:otherwise>
                    <SSCHString>
                        <xsl:value-of select="$Start/root/OrderStr"/>
                    </SSCHString>
                </xsl:otherwise>
            </xsl:choose>
        </root>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="SSCHString" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:endType>
    <pd:endX>745</pd:endX>
    <pd:endY>111</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1468216487592</pd:targetNamespace>
    <pd:activity name="Render Header">
        <pd:type>com.tibco.plugin.cobol.CCBRenderActivity</pd:type>
        <pd:resourceType>ae.activities.CCBRenderActivity</pd:resourceType>
        <pd:description>Only Header Record</pd:description>
        <pd:x>623</pd:x>
        <pd:y>110</pd:y>
        <config>
            <encoding>ASCII</encoding>
            <encodingDisp1>ASCII</encodingDisp1>
            <bigEndian>true</bigEndian>
            <multipleRecords>true</multipleRecords>
            <delimiter>New Line</delimiter>
            <fillerChars>Space</fillerChars>
            <truncate>true</truncate>
            <renderAs>string</renderAs>
            <ae.palette.cobolpalette.sharedProperties.copybook>/SharedResources/Copybook/SSCH/DMSHDR.cpy</ae.palette.cobolpalette.sharedProperties.copybook>
        </config>
        <pd:inputBindings>
            <Input>
                <DMSHDR-RECORD>
                    <DMSHDR-RECORD-ID>
                        <xsl:value-of select="&quot;000&quot;"/>
                    </DMSHDR-RECORD-ID>
                    <DMSHDR-TYPE-OF-TRANS>
                        <xsl:value-of select="&quot;SSC ORDERS&quot;"/>
                    </DMSHDR-TYPE-OF-TRANS>
                    <DMSHDR-SEQUENCE-NBR>
                        <xsl:value-of select="2000000 + $Query-Control/resultSet/Record[1]/VER_NUM"/>
                    </DMSHDR-SEQUENCE-NBR>
                    <DMSHDR-DATE-TIME>
                        <DMSHDR-DATE>
                            <xsl:value-of select="tib:format-date(&quot;yyyyMMdd&quot;, current-date())"/>
                        </DMSHDR-DATE>
                        <DMSHDR-TIME>
                            <xsl:value-of select="tib:format-time(&quot;HHmmss&quot;, substring-after(current-dateTime(), &quot;T&quot;))"/>
                        </DMSHDR-TIME>
                    </DMSHDR-DATE-TIME>
                    <DMSHDR-LOCATION-CODE>
                        <xsl:value-of select="&quot;DL&quot;"/>
                    </DMSHDR-LOCATION-CODE>
                    <DMSHDR-SYS-SOURCE-CODE>
                        <xsl:value-of select="&quot;RMS&quot;"/>
                    </DMSHDR-SYS-SOURCE-CODE>
                </DMSHDR-RECORD>
            </Input>
        </pd:inputBindings>
    </pd:activity>
    <pd:group name="Transaction">
        <pd:type>com.tibco.pe.core.TransactionGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>279</pd:x>
        <pd:y>32</pd:y>
        <pd:width>258</pd:width>
        <pd:height>141</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>transactionGroup</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:transactionType>JDBCTransaction</pd:transactionType>
            <pd:includeCheckpoint>false</pd:includeCheckpoint>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Query Control</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Update Control</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Query Control</pd:from>
            <pd:to>Update Control</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="Query Control">
            <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
            <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
            <pd:x>354</pd:x>
            <pd:y>108</pd:y>
            <config>
                <timeout>%%Connections/Database/RMS/DBTimeOut%%</timeout>
                <commit>false</commit>
                <maxRows>1</maxRows>
                <emptyStrAsNil>false</emptyStrAsNil>
                <statement>select ver_num from jcp_pisl_outb_ctrl where if_id = 'RMSPO_181_179_SSCH'</statement>
                <oraObjects/>
                <oraTables/>
                <QueryOutputCachedSchemaColumns>VER_NUM</QueryOutputCachedSchemaColumns>
                <QueryOutputCachedSchemaDataTypes>-5</QueryOutputCachedSchemaDataTypes>
                <QueryOutputCachedSchemaDataTypesName>number</QueryOutputCachedSchemaDataTypesName>
                <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
                <jdbcSharedConfig>/Configurations/Connections/Database/RMS.sharedjdbc</jdbcSharedConfig>
            </config>
            <pd:inputBindings>
                <jdbcQueryActivityInput/>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Update Control">
            <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
            <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
            <pd:x>454</pd:x>
            <pd:y>107</pd:y>
            <config>
                <timeout>%%Connections/Database/RMS/DBTimeOut%%</timeout>
                <commit>false</commit>
                <emptyStrAsNil>false</emptyStrAsNil>
                <statement>UPDATE JCPRMS.JCP_PISL_OUTB_CTRL
SET    VER_NUM = ?,
       LAST_UPDATE_TS = CURRENT_TIMESTAMP,
       LAST_UPDATE_ID = ?
WHERE IF_ID = 'RMSPO_181_179_SSCH'</statement>
                <Prepared_Param_DataType>
                    <parameter>
                        <parameterName>VER_NUM</parameterName>
                        <dataType>NUMERIC</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>LAST_UPDATE_ID</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                </Prepared_Param_DataType>
                <jdbcSharedConfig>/Configurations/Connections/Database/RMS.sharedjdbc</jdbcSharedConfig>
            </config>
            <pd:inputBindings>
                <jdbcUpdateActivityInput>
                    <xsl:choose>
                        <xsl:when test="$Query-Control/resultSet/Record[1]/VER_NUM = 999999">
                            <VER_NUM>
                                <xsl:choose>
                                    <xsl:when test="exists(0)">
                                        <xsl:value-of select="0"/>
                                    </xsl:when>
                                    <xsl:otherwise>
                                        <xsl:attribute name="xsi:nil">true</xsl:attribute>
                                    </xsl:otherwise>
                                </xsl:choose>
                            </VER_NUM>
                        </xsl:when>
                        <xsl:otherwise>
                            <VER_NUM>
                                <xsl:choose>
                                    <xsl:when test="exists($Query-Control/resultSet/Record[1]/VER_NUM + 1)">
                                        <xsl:value-of select="$Query-Control/resultSet/Record[1]/VER_NUM + 1"/>
                                    </xsl:when>
                                    <xsl:otherwise>
                                        <xsl:attribute name="xsi:nil">true</xsl:attribute>
                                    </xsl:otherwise>
                                </xsl:choose>
                            </VER_NUM>
                        </xsl:otherwise>
                    </xsl:choose>
                    <LAST_UPDATE_ID>
                        <xsl:value-of select="&quot;TIBCO&quot;"/>
                    </LAST_UPDATE_ID>
                </jdbcUpdateActivityInput>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="List File">
        <pd:type>com.tibco.plugin.file.ListFilesActivity</pd:type>
        <pd:resourceType>ae.activities.ListFilesActivity</pd:resourceType>
        <pd:x>178</pd:x>
        <pd:y>109</pd:y>
        <config>
            <mode>only-files</mode>
        </config>
        <pd:inputBindings>
            <pfx:ListFilesActivityConfig>
                <fileName>
                    <xsl:value-of select="$Start/root/FileName"/>
                </fileName>
            </pfx:ListFilesActivityConfig>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Null">
        <pd:type>com.tibco.plugin.timer.NullActivity</pd:type>
        <pd:resourceType>ae.activities.null</pd:resourceType>
        <pd:x>180</pd:x>
        <pd:y>247</pd:y>
        <config/>
        <pd:inputBindings/>
    </pd:activity>
    <pd:transition>
        <pd:from>Render Header</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>List File</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Transaction</pd:from>
        <pd:to>Render Header</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>List File</pd:from>
        <pd:to>Transaction</pd:to>
        <pd:xpathDescription>No File</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>count($List-File/pfx:ListFilesActivityOutput/files/fileInfo) = 0</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>List File</pd:from>
        <pd:to>Null</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Null</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>One Bend</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>
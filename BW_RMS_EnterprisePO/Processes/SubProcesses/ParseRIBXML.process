<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.oracle.com/retail/integration/rib/RibMessages" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns="http://www.tibco.com/pe/EngineTypes" xmlns:ns12="http://schema.jcpenney.com/schemas/BWLER/LogMessage" xmlns:BW="java://com.tibco.pe.core.JavaCustomXPATHFunctions" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:ns1="http://www.tibco.com/pe/DeployedVarsType">
    <xsd:import namespace="http://www.oracle.com/retail/integration/rib/RibMessages" schemaLocation="/SharedResources/Schemas/RibMessages.xsd"/>
    <pd:name>Processes/SubProcesses/ParseRIBXML.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="RIBXMLString" type="xsd:string"/>
                    <xsd:element name="JMSMSGID" type="xsd:string" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>150</pd:startX>
    <pd:startY>64</pd:startY>
    <pd:returnBindings>
        <root>
            <ErrorStr>
                <xsl:value-of select="tib:render-xml($_error/ns:ErrorReport)"/>
            </ErrorStr>
        </root>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="ErrorStr" type="xsd:string" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:endType>
    <pd:endX>450</pd:endX>
    <pd:endY>65</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1468577805972</pd:targetNamespace>
    <pd:activity name="Parse RIB XML">
        <pd:type>com.tibco.plugin.xml.XMLParseActivity</pd:type>
        <pd:resourceType>ae.activities.XMLParseActivity</pd:resourceType>
        <pd:x>302</pd:x>
        <pd:y>66</pd:y>
        <config>
            <inputStyle>text</inputStyle>
            <xsdVersion>default</xsdVersion>
            <validateOutput>true</validateOutput>
            <term ref="pfx:RibMessages"/>
        </config>
        <pd:inputBindings>
            <xmlString>
                <xsl:value-of select="$Start/root/RIBXMLString"/>
            </xmlString>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="POException">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>300</pd:x>
        <pd:y>180</pd:y>
        <config>
            <processName>/Processes/SubProcesses/POExceptionLog.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <Request_Type>
                    <xsl:value-of select="$Parse-RIB-XML/pfx:RibMessages/pfx:ribMessage[1]/pfx:type"/>
                </Request_Type>
                <Business_ID>
                    <xsl:value-of select="$Parse-RIB-XML/pfx:RibMessages/pfx:ribMessage[1]/pfx:id"/>
                </Business_ID>
                <ERROR_CODE>
                    <xsl:value-of select="$_error/ns:ErrorReport/MsgCode"/>
                </ERROR_CODE>
                <ERROR_DESC>
                    <xsl:value-of select="$_error/ns:ErrorReport/Msg"/>
                </ERROR_DESC>
                <INPUT_REQUEST>
                    <xsl:value-of select="$Start/root/RIBXMLString"/>
                </INPUT_REQUEST>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Submit_Exception_Unhandled">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>450</pd:x>
        <pd:y>178</pd:y>
        <config>
            <spawn>true</spawn>
            <processName>/CommonLoggingErrorReporting/Process Definitions/Logging/Logging.process</processName>
        </config>
        <pd:inputBindings>
            <ns12:LogMessage>
                <LogLevel>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/BWLER/LogLevels/glbERROR"/>
                </LogLevel>
                <ProcessId>
                    <xsl:value-of select="$_processContext/ns:ProcessContext/ProcessId"/>
                </ProcessId>
                <ProjectName>
                    <xsl:value-of select="$_processContext/ns:ProcessContext/ProjectName"/>
                </ProjectName>
                <ProcessName>
                    <xsl:value-of select="BW:getCurrentProcessName($_processContext/ns:ProcessContext/ProcessId)"/>
                </ProcessName>
                <xsl:if test="$Start/root/JMSMSGID">
                    <JMSMessageID>
                        <xsl:value-of select="$Start/root/JMSMSGID"/>
                    </JMSMessageID>
                </xsl:if>
                <MessageCode>
                    <xsl:value-of select="&quot;Exception&quot;"/>
                </MessageCode>
                <Message>
                    <xsl:value-of select="$_error/ns:ErrorReport/Msg"/>
                </Message>
                <Exception>
                    <ErrorReport>
                        <StackTrace>
                            <xsl:value-of select="$_error/ns:ErrorReport/StackTrace"/>
                        </StackTrace>
                        <Msg>
                            <xsl:value-of select="$_error/ns:ErrorReport/Msg"/>
                        </Msg>
                        <FullClass>
                            <xsl:value-of select="$_error/ns:ErrorReport/FullClass"/>
                        </FullClass>
                        <Class>
                            <xsl:value-of select="$_error/ns:ErrorReport/Class"/>
                        </Class>
                        <ProcessStack>
                            <xsl:value-of select="$_error/ns:ErrorReport/ProcessStack"/>
                        </ProcessStack>
                        <MsgCode>
                            <xsl:value-of select="901"/>
                        </MsgCode>
                        <xsl:copy-of select="$_error/ns:ErrorReport/Data"/>
                    </ErrorReport>
                    <SourceData>
                        <xsl:value-of select="'XML_PO_GlobalExceptionError --> Look for the error in AQ Procedure'"/>
                    </SourceData>
                </Exception>
            </ns12:LogMessage>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>149</pd:x>
        <pd:y>181</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Parse RIB XML</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Parse RIB XML</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>POException</pd:from>
        <pd:to>Submit_Exception_Unhandled</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Submit_Exception_Unhandled</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>POException</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>